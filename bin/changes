#!/bin/sh

TEMP=`getopt -o '' --long since: -- "$@"`
eval set -- "$TEMP"
while true
do
  case "$1" in
    --since)
      since=$2
      shift 2 ;;
    --)
      shift
      break ;;
  esac
done

if [ -e $since ]
  then
    changed=`find "$@" -newer $since`
    [ "$changed" != "" ]
  else
    true
fi
